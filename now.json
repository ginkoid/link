{
  "version": 1,
  "type": "docker",
  "features": {
    "cloud": "v2"
  },
  "scale": {
    "all": {
      "min": 0,
      "max": 5
    }
  },
  "env": {
    "APP_GITHUB_GIST_ID": "@app-link-github-gist-id",
    "APP_GITHUB_CLIENT_ID": "@app-link-github-client-id",
    "APP_GITHUB_CLIENT_SECRET": "@app-link-github-client-secret"
  },
  "alias": [
    "gnk.io",
    "www.gnk.io"
  ]
}
